<% videos ||= [] %>
<% sorted_videos = videos.sort_by{ |video| video.published_at }.reverse %>

<div class="row">
    <% sorted_videos.each do |video| %>
        <div class="col-lg-3 col-md-4 mb-2 pl-1 pr-1">
            <%= render partial: "youtube/video", locals: { video: video } %>
        </div>
    <% end %>    
</div>
