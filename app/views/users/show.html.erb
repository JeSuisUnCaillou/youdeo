<h1><%= @user.name %></h1>

<% if user_signed_in? && @user == current_user %>
    <p>(this is you) <%= @elapsed_time %>s</p>
    
    
    <%= render partial: "tags/tags", locals: { tags: @tags } %>

    <h2>Channels without tags <small>(<%= @uncategorized_channels.count %>)</small></h2>
    
    <%= render partial: "youtube/channels", locals: { channels: @uncategorized_channels.values } %>
    
    
    
<% end %>